{{#each propertyData}}
  <a
    class='property-card card col-sm-12 col-md-5 col-lg-2 m-3 augmentate-1'
    href='/about-property/{{id}}'
    property-id='{{id}}'
  >
    <div id='bookmark-icon'><i class='fa-solid fa-heart'></i></div>
    <img
      class='card-img-top'
      src='https://source.unsplash.com/random/1080x720/?house&{{@index}}1'
      alt='Card image cap'
    />
    <div class='d-flex flex-column p-2'>
      <li class='list-unstyled'>{{city}}, {{address}} </li>
      <li class='ms-auto list-unstyled'>Â£ {{price}} p/m</li>
    </div>
  </a>
{{/each}}